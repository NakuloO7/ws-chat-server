

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Message{
  id String @id @default(uuid())
  roomId String
  content String

  userId String
  userName String
  
  createdAt DateTime @default(now())

  edited Boolean @default(false)
  deleted Boolean @default(false)
}

model User { 
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  createdAt DateTime @default(now())
}